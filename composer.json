{
    "name": "grizz-it/php-ast-generator",
    "description": "A set of generators for creating PHP classes and interfaces.",
    "keywords": [
        "generator",
        "ast",
        "reflection"
    ],
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "grizz-it/ast": "^1.3"
    },
    "authors": [
        {
            "name": "Grizz-IT",
            "homepage": "https://www.grizz-it.com/",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "GrizzIt\\PhpAstGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrizzIt\\PhpAstGenerator\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            "/.gitignore",
            "/.travis.yml",
            "/phpunit.xml",
            "/phpcs.xml",
            "/PULL_REQUEST_TEMPLATE.md",
            "/CODE_OF_CONDUCT.md",
            "/CONTRIBUTING.md"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
